<% include ../partials/head %>
<h1><%=title%></h1>
<table class='table table-striped table-bordered'>
    <tr>
        <th>hw_addr</th>
        <th>state</th>
        <th>timeout</th>
        <th>creator</th>
        <th>owner</th>
    </tr>
    <% leases.forEach(function(lease) { %>
    <tr>
        <td><%= lease.hw_addr %></td>
        <td><%= lease.state %></td>
        <td><%= lease.timeout %></td>
        <td><%= lease.creator %></td>
        <td><%= lease.owner %></td>
    </tr>
    <% }); %>

</table>
<% include ../partials/foot %>